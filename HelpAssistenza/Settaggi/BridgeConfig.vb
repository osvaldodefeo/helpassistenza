Module BridgeConfig
    Public Sub trasportaNomeAzienda()
        frmAssistenza.lblNomeAzienda.Text = frmConfigurazione.txtnomeazienda.Text
        frmAssistenza.LinkAziendaAss.Text = frmConfigurazione.txtsitointernet.Text
        If frmConfigurazione.txtlogo.Text = "DEFAULTLOGO" Then
            frmAssistenza.picLogoAzienda.ImageLocation = Application.StartupPath + "\Logo.png"
            frmConfigurazione.txtlogo.Enabled = False
        ElseIf frmConfigurazione.txtlogo.Text <> String.Empty And frmConfigurazione.txtlogo.Text <> "Percorso Logo" Then
            frmAssistenza.picLogoAzienda.ImageLocation = frmConfigurazione.txtlogo.Text
            frmConfigurazione.txtlogo.Enabled = True
        End If
        If frmConfigurazione.txtnomeazienda.Text <> "Nome Azienda" Then
            frmAssistenza.ToolTipTeleassistenza.ToolTipTitle = "Teleassistenza " & frmConfigurazione.txtnomeazienda.Text
        End If
        If frmConfigurazione.txtnomeazienda.Text <> "Nome Azienda" And frmConfigurazione.txtnomeazienda.Text <> String.Empty Then
            frmAssistenza.Text = "HelpAssistenza - " & frmConfigurazione.txtnomeazienda.Text
        End If
    End Sub

