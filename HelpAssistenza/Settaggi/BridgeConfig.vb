    Sub ControllaGeneraPathTeleControllo()
        If frmConfigurazione.chkGeneraPathTeleControllo.Checked = True Then
            Dim pathAttuale As String = frmConfigurazione.txtPathTeleControllo.Text
            If pathAttuale <> String.Empty Then pathAttuale = pathAttuale.Remove(pathAttuale.Length - 1)
            Dim stringen As New RandomKeyGenerator
            stringen.KeyLetters = "abcdefghijklmnopqrstuvwxyz"
            stringen.KeyNumbers = "0123456789"
            stringen.KeyChars = 6
            Dim RandomString As String = stringen.Generate
            Dim FTPath As String = pathAttuale + _
            "/" + My.Computer.Name + "_" + RandomString + "/"
            frmConfigurazione.txtPathTeleControllo.Text = FTPath
            frmConfigurazione.chkGeneraPathTeleControllo.Checked = False
            Dim slvIniFile As New INIFiles(Application.StartupPath & "\HelpAssistenza.ini")
            slvIniFile.WriteString("TELECONTROLLO", "GENERA_PATH", "False")
        End If
