    Sub ControllaEsistenzaEseguibili()
        'Controllo se presenti File dei Programmi Collegamento
        If CheckEXE = 1 Then
            Dim pathvnc As String = frmConfigurazione.txtpercorsoadsl.Text
            Dim pathPcAny As String = frmConfigurazione.txtpercorsoadsl2.Text
            Dim pathModem As String = frmConfigurazione.txtpercorsomodem.Text
            If pathvnc.Contains("%PROGRAMPATH%") Then pathvnc = Replace(pathvnc, "%PROGRAMPATH%", Application.StartupPath)
            If pathvnc.Contains("%PROGRAMFILES%") Then pathvnc = Replace(pathvnc, "%PROGRAMFILES%", My.Computer.FileSystem.SpecialDirectories.ProgramFiles)
            If pathPcAny.Contains("%PROGRAMPATH%") Then pathPcAny = Replace(pathPcAny, "%PROGRAMPATH%", Application.StartupPath)
            If pathPcAny.Contains("%PROGRAMFILES%") Then pathPcAny = Replace(pathPcAny, "%PROGRAMFILES%", My.Computer.FileSystem.SpecialDirectories.ProgramFiles)
            If pathModem.Contains("%PROGRAMPATH%") Then pathModem = Replace(pathModem, "%PROGRAMPATH%", Application.StartupPath)
            If pathModem.Contains("%PROGRAMFILES%") Then pathModem = Replace(pathModem, "%PROGRAMFILES%", My.Computer.FileSystem.SpecialDirectories.ProgramFiles)

            If (frmConfigurazione.txtpercorsoadsl.Text <> String.Empty And frmConfigurazione.txtpercorsoadsl2.Text <> String.Empty And frmConfigurazione.txtpercorsomodem.Text <> String.Empty) And (My.Computer.FileSystem.FileExists(pathvnc) = False And My.Computer.FileSystem.FileExists(pathPcAny) = False And My.Computer.FileSystem.FileExists(pathModem) = False) Then
                frmConfigurazione.chkvnc.Checked = False
                frmConfigurazione.chkmodem.Checked = False
                frmAssistenza.Groupline.Enabled = False
                MessageBox.Show("Verificare Esistenza Programmi di Collegamento !!!", "HelpAssistenza", MessageBoxButtons.OK, MessageBoxIcon.Warning)
                Exit Sub
            ElseIf (frmConfigurazione.txtpercorsoadsl.Text <> String.Empty And frmConfigurazione.txtpercorsoadsl2.Text <> String.Empty) And (My.Computer.FileSystem.FileExists(pathvnc) = False And My.Computer.FileSystem.FileExists(pathPcAny) = False) Then
                frmConfigurazione.chkvnc.Checked = False
                MessageBox.Show("Verificare Esistenza Programmi di Collegamento !!!", "HelpAssistenza", MessageBoxButtons.OK, MessageBoxIcon.Warning)
                Exit Sub
            ElseIf frmConfigurazione.txtpercorsoadsl2.Text <> String.Empty And My.Computer.FileSystem.FileExists(pathPcAny) = False Then
                MessageBox.Show("Verificare Esistenza Programmi di Collegamento !!!", "HelpAssistenza", MessageBoxButtons.OK, MessageBoxIcon.Warning)
                Exit Sub
            ElseIf frmConfigurazione.txtpercorsoadsl.Text <> String.Empty And My.Computer.FileSystem.FileExists(pathvnc) = False Then
                MessageBox.Show("Verificare Esistenza Programmi di Collegamento !!!", "HelpAssistenza", MessageBoxButtons.OK, MessageBoxIcon.Warning)
                Exit Sub
            ElseIf frmConfigurazione.txtpercorsomodem.Text <> String.Empty And My.Computer.FileSystem.FileExists(pathModem) = False Then
                MessageBox.Show("Verificare Esistenza Programmi di Collegamento !!!", "HelpAssistenza", MessageBoxButtons.OK, MessageBoxIcon.Warning)
            End If
        End If
    End Sub
    Public Function ControllaModem2()
        Dim pathModem As String = frmConfigurazione.txtpercorsomodem.Text
        If pathModem.Contains("%PROGRAMPATH%") Then pathModem = Replace(pathModem, "%PROGRAMPATH%", Application.StartupPath)
        If pathModem.Contains("%PROGRAMFILES%") Then pathModem = Replace(pathModem, "%PROGRAMFILES%", My.Computer.FileSystem.SpecialDirectories.ProgramFiles)
        If My.Computer.FileSystem.FileExists(pathModem) = True Then
            Return False
        Else
            frmAssistenza.Operatore = 0
            MessageBox.Show("Impossibile Trovare eseguibile del Programma Collegamento Modem!" + _
                            vbCrLf + "Verificare la corretta installazione del Programma Collegamento Modem", "HelpAssistenza", _
                            MessageBoxButtons.OK, MessageBoxIcon.Error)
            Return True
        End If
    End Function
